{#
/**
 * @file ever_entity.html.twig
 * Default theme implementation to present Ever entity data.
 *
 * This template is used when viewing Ever entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_ever_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('ever_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
